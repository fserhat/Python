print("ikinci dereceden denklem kökü bulma programı")
print("ax**2 + b*x +c")
a = int(input("birinci değer:"))
b = int(input("ikinci değer:"))
c = int(input("ücüncü değer:"))

delta = b**2 - 4*a*c

x1 = (-b - delta ** 0.5) / (2*a)
x2 = (-b + delta ** 0.5) / (2*a)

print("birinci kök: {}\nİkinci kök: {}\n:".format(x1,x2))
